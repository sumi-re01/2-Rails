<h1>編集画面</h1>
<%= form_with model:@list, update_todolist_path(@list.id), local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <h4>画像</h4>
  <%= attachment_image_tag @list, :image, format: 'jpeg', size: "100x100" %>
  <%= f.attachment_field :image %>
  <%= f.submit '投稿' %>
<% end %>